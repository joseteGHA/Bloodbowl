                        <![CDATA[<i>Note:</i> uploading in photo numbers already occupied will overwrite the current photo in that position.]]>
                        </pnote>
                </report>
                
            </fixtures>
            
            <standings>
            
                <!-- General -->
                <tblTitle>Overall standings</tblTitle>
                <tblTitle2>Overall standings continued (team details)</tblTitle2>
